{
    "include": ["src"],
    "compilerOptions": {
      "target": "es2020",
      "module": "esnext",
      "declaration": true,
      "outDir": "dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "jsx": "react-jsx",
      "moduleResolution": "Node"
    }
  }